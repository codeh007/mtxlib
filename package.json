{
  "name": "mtxlib",
  "private": false,
  "version": "0.6.2",
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "files": ["dist", "src"],
  "types": "./dist/index.d.ts",
  "main": "./dist/index.js",
  "typesVersions": {
    "*": {
      "*": [
        "src/*.ts",
        "src/*.tsx",
        "src/*/index.ts",
        "src/*/index.tsx",
        "dist/*.d.ts",
        "dist/*",
        "dist/*/index"
      ]
    }
  },
  "exports": {
    ".": {
      "types": ["./src/index.ts", "./dist/index.js", "./dist/index.d.ts"],
      "import": ["./src/index.ts", "./dist/index.js", "./dist/*"]
    },
    "./*": {
      "types": [
        "./src/*",
        "./src/*.tsx",
        "./src/*.ts",
        "./src/*/index.ts",
        "./src/*.tsx",
        "./src/*/index.tsx",
        "./dist/*.d.ts",
        "./dist/*/index.d.ts"
      ],
      "import": [
        "./src/*",
        "./src/*.ts",
        "./src/*.tsx",
        "./src/*/index.ts",
        "./src/*/index.tsx",
        "./dist/*/index.js",
        "./dist/*.js",
        "./dist/*"
      ],
      "default": [
        "./src/*",
        "./src/*.ts",
        "./src/*.tsx",
        "./src/*/index.ts",
        "./src/*/index.tsx",
        "./dist/*/index.js",
        "./dist/*.js",
        "./dist/*"
      ]
    },
    "./styles/*": {
      "import": ["./src/styles/*", "./dist/styles/*"],
      "require": ["./src/styles/*", "./dist/styles/*"]
    }
  },
  "scripts": {
    "dev": "npx next dev",
    "build": "tsc --outDir dist --jsx react-jsx --incremental false --declaration true --noEmit false --removeComments true --project ",
    "db:migrate": "bun run src/db/migrate.ts",
    "gen": "bun src/cli/main.ts gen"
  },
